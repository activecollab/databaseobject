{
    "name": "activecollab/databaseobject",
    "type": "library",
    "description": "Library that enables easy persistance of objects to the MySQL 5.7+ database",
    "license": "MIT",
    "keywords": [
        "persistence",
        "mysql",
        "activecollab"
    ],
    "homepage": "https://labs.activecollab.com",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@activecollab.com"
        }
    ],
    "config": {
        "platform": {
            "php": "8.2.0"
        }
    },
    "require": {
        "php": ">=8.2",
        "activecollab/databaseconnection": "^5.0",
        "activecollab/etag": "^2.0",
        "activecollab/object": "^1.0",
        "doctrine/inflector": "^2.0",
        "psr/log": "^1.0.0"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "friendsofphp/php-cs-fixer": "^2.0",
        "monolog/monolog": "^1.0",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\DatabaseObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\DatabaseObject\\Test\\": "test/src"
        }
    }
}
